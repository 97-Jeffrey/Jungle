
<header class="page-header">
<h1> All Categories</h1>
</header>

<div class="well">
    <%= link_to '+ New Category', [:new, :admin, :category], class: 'btn btn-info' %>
  </div>


  <table class="table table-bordered">
    <thead>
     <tr>
      <th> All Categories</th>
     </tr>
    </thead>
    <tbody>
      <% @categories.each do |category|%>
       <tr>
        <td><%=category.name%></td>
       </tr>
      <%end%>
    </tbody>
  
  </table>